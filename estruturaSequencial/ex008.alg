algoritmo "06042022ex008"
var
   porHora, horasTrab, salario : inteiro
inicio
      escreval("quanto voce ganha por hora e quantas horas voce trabalha por mes ?")
      leia(porHora)
      leia(horasTrab)
      salario <- porHora * horasTrab
      escreva("seu salario é:", salario)
      
fimalgoritmo
