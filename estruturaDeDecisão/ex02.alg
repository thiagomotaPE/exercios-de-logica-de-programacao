algoritmo "ex02"
var
   val : real
inicio
      escreval("digite um numero :")
      leia(val)

      se (val >= 0) entao
         escreval("este numero é positivo")
      senao
           escreval("este numero é negativo")
      fimse

      se (val = 0) entao
         escreva("este é um numero neutro")
      fimse
fimalgoritmo
